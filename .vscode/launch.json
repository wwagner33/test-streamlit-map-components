//executa o streamlit no arquivo atual no modo de depuração
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Streamlit: Current File",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "${file}"],
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
